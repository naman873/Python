    "class Solution:\n",
    "    def majorityElement(self, nums):\n",
    "        ans,count=np.unique(nums,return_counts=True)\n",
    "        top=np.argmax(count)\n",
    "        a=ans[top]\n",
    "        return a"
   ]
  },
